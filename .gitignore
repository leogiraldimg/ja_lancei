# Ignore all logfiles and tempfiles.
src/ja_lancei/log/*
src/ja_lancei/tmp/*
!src/ja_lancei/log/.keep
!src/ja_lancei/tmp/.keep

# Ignore uploaded files in development
src/ja_lancei/storage/*
!src/ja_lancei/storage/.keep

src/ja_lancei/node_modules
src/ja_lancei/yarn-error.log

src/ja_lancei/public/assets
src/ja_lancei.byebug_history

# Ignore master key for decrypting credentials and more.
src/ja_lancei/config/master.key

# Ignore gmail credentials
src/ja_lancei/.env
